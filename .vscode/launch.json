      "mode": "auto",
      "program": "${workspaceFolder}/cmd/echo-example",
      "env": {
        "APPLICATION_NAME": "example",
        "APPLICATION_ENVIRONMENT": "Development",
        "LOG_LEVEL": "debug",
        "PRETTY_LOG": "true",
        "PORT": "9044"
      },
      "args": []
