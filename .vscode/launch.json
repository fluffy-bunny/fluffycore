    {
      "name": "example app - serve",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/example/server",
      "env": {
        "EXAMPLE_APPLICATION_NAME": "example",
        "EXAMPLE_APPLICATION_ENVIRONMENT": "Development",
        "APPLICATION_ENVIRONMENT": "Development",
        "EXAMPLE_LOG_LEVEL": "debug",
        "EXAMPLE_PRETTY_LOG": "true",
        "EXAMPLE_PORT": "50051",
        "GRPC_GATEWAY_ENABLED": "true",
        "EXAMPLE_REST_PORT": "50052",
        "EXAMPLE_oauth2Port": "50053",
        "EXAMPLE_customString": "In Flames",
        "EXAMPLE_someSecret": "1234567890",
        "ENABLE_GRPC_SERVER_REFLECTION": "true",
        "EXAMPLE_jwtValidators__issuers": "http://localhost:50053,http://host.docker.internal:50053",
        "EXAMPLE_jwtValidators__jwksUrls": "http://localhost:50053/.well-known/jwks,http://host.docker.internal:50053/.well-known/jwks"
      },
      "args": ["serve"]
    },
    {
      "name": "example app - version",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/example/server",
      "env": {
        "APPLICATION_NAME": "example",
        "APPLICATION_ENVIRONMENT": "Development",
        "LOG_LEVEL": "debug",
        "PRETTY_LOG": "true",
        "PORT": "50051",
        "GRPC_GATEWAY_ENABLED": "true",
        "REST_PORT": "50052",
        "GRPCPort": "50051",
        "customString": "In Flames",
        "someSecret": "1234567890"
      },
      "args": ["version"]
    },
    {
      "name": "Server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/server",
      "args": ["serve"]
    },
    {
      "name": "helloworld - Server",
      "type": "go",
